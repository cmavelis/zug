{
  "name": "simulchess",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "build-watch": "vite build --watch",
    "build:dev": "vite build --mode development",
    "build-watch:dev": "vite build --watch --mode development",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check-watch": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false -w",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "server:dev": "nodemon server/main.ts --watch server/main.ts --watch src/",
    "server:prod": "ts-node server/main.ts",
    "start": "npm run server:prod"
  },
  "dependencies": {
    "bgio-postgres": "^1.0.15",
    "boardgame.io": "^0.50.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "koa-body": "^6.0.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "primeicons": "^6.0.1",
    "primevue": "^3.42.0",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.0",
    "ts-node": "^10.9.1",
    "vue": "^3.3.6",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/axios": "^0.14.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/koa": "^2.13.12",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.19.2",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "axios": "^1.6.0",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-vue-type-imports": "^0.2.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.20"
  }
}
