{
  "name": "simulchess",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "build-watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check-watch": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false -w",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "server:dev": "nodemon server/main.ts --watch server/main.ts --watch src/",
    "server:prod": "ts-node server/main.ts",
    "start": "npm run server:prod"
  },
  "dependencies": {
    "boardgame.io": "^0.50.2",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "vue": "^3.3.6",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.2.2",
    "vite": "^3.1.8",
    "vite-plugin-vue-type-imports": "^0.2.4",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.8.20"
  }
}
